    "ws": "^7.4.2"
  },
  "devDependencies": {
    "airtap": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "babel-minify": "^0.5.1",
    "browserify": "^17.0.0",
    "prettier-bytes": "^1.0.4",
    "speedometer": "^1.1.0",
    "standard": "*",
    "tape": "^5.1.1"
  },
  "homepage": "https://github.com/feross/simple-websocket",
