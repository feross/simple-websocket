    "debug": "^4.3.4",
    "queue-microtask": "^1.2.3",
    "streamx": "^2.13.2",
    "uint8-util": "^2.1.6",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "airtap": "^4.0.4",
    "airtap-manual": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "babel-minify": "^0.5.2",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "prettier-bytes": "^1.0.4",
    "speedometer": "^1.1.0",
    "standard": "*",
    "tape": "^5.6.3"
  },
  "homepage": "https://github.com/feross/simple-websocket",
  "keywords": [
    "simple",
    "simple websocket",
    "simple-websocket",
    "socket",
    "websocket",
    "ws"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/feross/simple-websocket.git"
  },
  "scripts": {
    "build": "browserify -s SimpleWebsocket -r . | minify > simplewebsocket.min.js",
    "size": "npm run build && cat simplewebsocket.min.js | gzip | wc -c",
    "test": "standard && npm run test-node && npm run test-browser",
    "test-browser": "airtap -- test/*.js",
    "test-browser-local": "airtap --preset local -- test/*.js",
    "test-node": "tape test/*.js test/node/*.js"
  },
  "standard": {
    "parser": "babel-eslint"
  },
