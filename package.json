{
  "name": "simple-websocket",
  "description": "Simple, EventEmitter API for WebSockets (browser)",
  "version": "4.0.1",
  "author": {
    "name": "Feross Aboukhadijeh",
    "email": "feross@feross.org",
    "url": "http://feross.org/"
  },
  "browser": {
    "ws": false
  },
  "bugs": {
    "url": "https://github.com/feross/simple-websocket/issues"
  },
  "dependencies": {
    "debug": "^2.1.3",
    "inherits": "^2.0.1",
    "readable-stream": "^2.0.5",
    "ws": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "standard": "^6.0.4",
    "tape": "^4.0.0",
    "uglify-js": "^2.4.15",
    "zuul": "^3.0.0"
  },
  "homepage": "https://github.com/feross/simple-websocket",
  "keywords": [
    "simple",
    "simple websocket",
    "simple-websocket",
    "socket",
    "websocket",
    "ws"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/feross/simple-websocket.git"
  },
  "scripts": {
    "build": "browserify -s SimpleWebsocket -r ./ | uglifyjs -c warnings=false -m > simplewebsocket.min.js",
    "test": "standard && npm run test-node && npm run test-browser",
    "test-browser": "zuul -- test/*.js",
    "test-browser-local": "zuul --local -- test/*.js",
    "test-node": "tape test/*.js"
  }
